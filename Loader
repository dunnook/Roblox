if not getgenv then
    function getgenv() return _G end
end
local UniverseId = game.GameId
local GameName = ""
if UniverseId == 7028566528  then
    GameName = "basketballzero"
elseif UniverseId == 6884266247 then
    GameName = "animerangersx"
elseif UniverseId == 4495671366 then
    GameName = "standpower"
end
if GameName then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dunnook/Roblox/refs/heads/main/"..GameName))()
else
    print("Game is not supported by stagnant hub")
end
